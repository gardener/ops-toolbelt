alias pwru="pwru_install"
function pwru_install() {
  app="pwru"
  install_func='/nonroot/hacks/install_pwru'

  if ! hash "${app}" &>/dev/null; then
    "${install_func}" 1>&2
  fi

  command "${app}" "$@"
}
